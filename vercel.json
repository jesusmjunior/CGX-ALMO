{
  "version": 2,
  "name": "stock-control-system",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/backend/(.*)",
      "dest": "/api/backend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@backend_url",
    "GEMINI_API_KEY": "@gemini_key",
    "DATAWORLD_TOKEN": "@dataworld_token"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "@backend_url"
    }
  },
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "regions": ["gru1"],
  "framework": "nextjs"
}
